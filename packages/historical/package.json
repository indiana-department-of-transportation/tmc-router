{
  "name": "js-historical",
  "version": "0.0.1",
  "description": "API for interacting with react-router-dom history",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "http://jsmith@bitbucket.indot.org/scm/util/js-historical.git"
  },
  "keywords": [
    "react",
    "react-router",
    "history"
  ],
  "author": "jarsmith@indot.in.gov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "react": "^16.9.0",
    "react-router": "^5.0.1",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/index.ts -o dist/index.js --extensions \".ts,.tsx\" --source-maps inline",
    "build:ts": "tsc -p ./src --outDir ./dist",
    "gen-docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out doc ./src"
  }
}
